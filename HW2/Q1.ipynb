    "import hashlib\n",
    "\n",
    "\n",
    "user_input = input(\"Please enter the string: \")\n",
    "\n",
    "\n",
    "sha3_256 = hashlib.sha3_256()\n",
    "sha3_256.update(user_input.encode('utf-8'))\n",
    "hash_result = sha3_256.hexdigest()\n",
    "\n",
    "\n",
    "print(\"The result of SHA3-256 is :\", hash_result)"
